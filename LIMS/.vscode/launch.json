{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug LIMS",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/LIMS",
            "args": ["3", "40", "32", "0"],
            //"args": ["2", "3", "2", "1", "1", "10", "1"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}